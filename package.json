{
  "name": "quote-it",
  "description": "Your go-to API for a variety of quotes.",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "pnpm build && pnpm start",
    "format": "prettier --write .",
    "lint": "eslint src --ext .cjs,.js,.mjs,.ts --fix",
    "seed": "tsc && node --env-file=.env dist/util/seed.js",
    "start": "node --env-file=.env dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "@types/bad-words": "^3.0.3",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.28",
    "bad-words": "^3.0.4",
    "eslint": "^8.57.0",
    "eslint-config-neon": "^0.1.59",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "typescript": "^5.4.2"
  }
}
