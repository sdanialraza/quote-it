{
  "name": "quote-it",
  "description": "Your go-to API for a variety of quotes.",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "pnpm build && pnpm start",
    "format": "prettier --write .",
    "lint": "eslint src --ext .cjs,.js,.mjs,.ts --fix",
    "seed": "tsc && node --env-file=.env dist/util/seed.js",
    "start": "node --env-file=.env dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.8",
    "eslint": "^8.56.0",
    "eslint-config-neon": "^0.1.57",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "prisma": "^5.8.0",
    "typescript": "^5.3.3"
  }
}
